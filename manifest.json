{
  "manifest_version": 3,
  "name": "후니의 잡동사니",
  "version": "1.0",
  "description": "혼자 쓰려고 만든 도구 모음",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "features/ad-blocker/content.js"
      ]
    }
  ]
}
